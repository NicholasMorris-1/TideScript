peptide pepa = <SADCLKSCLRMCPDRIIL>;

molecule molb = (C14H31N7O8S);

solvent dmf;

#comment with a space

solution solx = [pepa(0.1 mM)] in [dmf];

solution soly = [molb (0.1 mM)] in [dmf];


 protocol <solution> conjugate (pepsol molsol) {
         solution z = combine pepsol and molsol;
         return z

};

call conjugate solx soly
