#declare peptide
peptide pepa = <SADCLKSCLRMCPDRIIL>;


#declare molecule
molecule molb = (C14H31N7O8S);

#declare solvents
solvent dmf;

#init solutions by passing in materials
solution solx = [pepa(2.0 mM)] in [dmf];
solution soly = [molb(2.87 mM)] in [dmf];

protocol <solution> conjugate (pepsol molsol volume unit){
    solution newsolution = mix pepsol (1.0 eq) + molsol (3.0 eq) at volume unit;
    #changeTemp newsolution to 21.0 C;
    agitate newsolution;
    wait for 3 hours;
    return newsolution
};

solution sola = call conjugate solx soly 1.0 mL
