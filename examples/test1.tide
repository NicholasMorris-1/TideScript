#declare peptide
peptide pepa = <SADCLKSCLRMCPDRIIL>;

#declare molecule
molecule molb = (C14H31N7O8S);

#declare solvent
solvent dmf;

#declare solutions by passing in materials
solution solx = [pepa(2.0 mM)] in [dmf];

solution soly = [molb(2.87 mM)] in [dmf];

print;

protocol <solution> conjugate (pepsol molsol volume){
         solution solz = mix pepsol (1.0 eq) + molsol (3.0 eq) at volume mL;
         agitate solz;
         changeTemp solz to 21.0 C;
         return solz
};

#call conjugate solx soly 5.0;

print;

solution solz = call conjugate solx soly 5.0;

print
